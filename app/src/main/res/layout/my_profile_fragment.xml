                        android:layout_toStartOf="@+id/student_my_profile_name_ar_logo"
                        android:text="الاسم بالعربيه" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/student_my_profile_name_en_logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
