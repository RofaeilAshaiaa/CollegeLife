                        android:layout_toStartOf="@+id/student_my_profile_major_logo"
                        android:text="لتخصص الرئيسي" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/student_my_profile_minor_logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="25dp"
                        android:background="@drawable/text_logo_background"
                        android:text="Minor"
                        android:textColor="@android:color/white"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/student_my_profile_minor_val"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_toLeftOf="@+id/student_my_profile_minor_logo"
                        android:layout_toStartOf="@+id/student_my_profile_minor_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="24sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_minor_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_minor_val"
                        android:layout_toLeftOf="@+id/student_my_profile_minor_logo"
                        android:layout_toStartOf="@+id/student_my_profile_minor_logo"
                        android:text="التخصص الفرعي" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/student_my_profile_academic_advisor_logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="25dp"
                        android:background="@drawable/text_logo_background"
                        android:text="Academic\n  advisor"
                        android:textColor="@android:color/white"
                        android:textSize="10sp" />

                    <TextView
                        android:id="@+id/student_my_profile_academic_advisor_val"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:layout_toLeftOf="@+id/student_my_profile_academic_advisor_logo"
                        android:layout_toStartOf="@+id/student_my_profile_academic_advisor_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="24sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_academic_advisor_text"
                        android:layout_width="wrap_content"
