                    <TextView
                        android:id="@+id/student_my_profile_address_val"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:layout_toLeftOf="@+id/student_my_profile_address_logo"
                        android:layout_toStartOf="@+id/student_my_profile_address_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="21sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_address_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_address_val"
