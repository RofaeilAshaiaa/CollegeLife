                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:layout_toLeftOf="@+id/student_my_profile_name_en_logo"
                        android:layout_toStartOf="@+id/student_my_profile_name_en_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="24sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_name_en_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_name_en_val"
                        android:layout_toLeftOf="@+id/student_my_profile_name_en_logo"
                        android:layout_toStartOf="@+id/student_my_profile_name_en_logo"
                        android:text="الاسم بالانجليزيه" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/student_my_profile_student_id_logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="25dp"
                        android:layout_marginTop="25dp"
                        android:background="@drawable/text_logo_background"
                        android:text="ID"
                        android:textColor="@android:color/white"
                        android:textSize="17sp" />

                    <TextView
                        android:id="@+id/student_my_profile_student_id_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_toLeftOf="@+id/student_my_profile_student_id_logo"
                        android:layout_toStartOf="@+id/student_my_profile_student_id_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="24sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_student_id_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_student_id_num"
                        android:layout_toLeftOf="@+id/student_my_profile_student_id_logo"
                        android:layout_toStartOf="@+id/student_my_profile_student_id_logo"
                        android:text="الرقم الجامعي" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/student_my_profile_branch_logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="25dp"
                        android:background="@drawable/text_logo_background"
                        android:text="Branch"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/student_my_profile_branch_val"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_toLeftOf="@+id/student_my_profile_branch_logo"
                        android:layout_toStartOf="@+id/student_my_profile_branch_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="24sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_branch_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_branch_val"
                        android:layout_toLeftOf="@+id/student_my_profile_branch_logo"
                        android:layout_toStartOf="@+id/student_my_profile_branch_logo"
                        android:text="الشعبة" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/student_my_profile_major_logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="25dp"
                        android:background="@drawable/text_logo_background"
                        android:text="Major"
                        android:textColor="@android:color/white"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/student_my_profile_major_val"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_toLeftOf="@+id/student_my_profile_major_logo"
                        android:layout_toStartOf="@+id/student_my_profile_major_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="24sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_major_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_major_val"
