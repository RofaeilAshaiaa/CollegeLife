<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true">

        <android.support.design.widget.AppBarLayout
            android:id="@+id/student_my_profile_app_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="280dp"
            android:fitsSystemWindows="true">

            <android.support.design.widget.CollapsingToolbarLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fitsSystemWindows="true"
                app:collapsedTitleGravity="left"
                app:contentScrim="@color/colorPrimary"
                app:layout_scrollFlags="scroll|snap|exitUntilCollapsed">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="0dp">

                    <TextView
                        android:id="@+id/student_my_profile_header_name_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="24sp" />

                    <TextView
                        android:id="@+id/student_my_profile_header_name_logo"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_below="@+id/student_my_profile_header_name_text"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/old_id_logo_background"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@android:color/white"
                        android:textSize="35sp" />

                    <ProgressBar
                        android:id="@+id/student_my_profile_header_earned_hours_progress_bar"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_below="@+id/student_my_profile_header_name_text"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="11.5dp"
                        android:background="@drawable/student_head_logo_progress_bar_circle_shape_background"
                        android:indeterminate="false"
                        android:max="140"
                        android:progress="0"
                        android:progressDrawable="@drawable/student_head_logo_progress_bar_circular_color" />

                    <ProgressBar
                        android:id="@+id/student_my_profile_header_cgpa_progress_bar"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="97dp"
                        android:layout_height="97dp"
                        android:layout_below="@+id/student_my_profile_header_name_text"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/student_head_logo_progress_bar_circle_shape_background"
                        android:indeterminate="false"
                        android:max="4"
                        android:progress="0"
                        android:progressDrawable="@drawable/student_head_logo_progress_bar_circular_color" />

                    <TextView
                        android:id="@+id/student_my_profile_header_level_val"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_header_name_text"
                        android:layout_marginTop="4dp"
                        android:layout_toEndOf="@+id/student_my_profile_header_cgpa_progress_bar"
                        android:layout_toRightOf="@+id/student_my_profile_header_cgpa_progress_bar"
                        android:background="@drawable/student_head_level_background"
                        android:text="0"
                        android:textColor="@android:color/white"
                        android:textSize="15sp" />

                </RelativeLayout>


            </android.support.design.widget.CollapsingToolbarLayout>

        </android.support.design.widget.AppBarLayout>

        <android.support.v4.widget.NestedScrollView
            android:id="@+id/student_my_profile_body_nested_scroll_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="35dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/student_my_profile_cgpa_logo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/text_logo_background"
                                android:text="CGPA"
                                android:textColor="@android:color/white"
                                android:textSize="18sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/student_my_profile_cgpa_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/student_my_profile_cgpa_logo"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:layout_marginTop="32dp"
                                android:text="0"
                                android:textColor="@android:color/black"
                                android:textSize="24sp" />

                            <ProgressBar
                                android:id="@+id/student_my_profile_cgpa_progress_bar"
                                style="?android:attr/progressBarStyleHorizontal"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:layout_below="@+id/student_my_profile_cgpa_logo"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/student_profile_body_progress_bar_circle_shape_background"
                                android:indeterminate="false"
                                android:max="4"
                                android:progress="0"
                                android:progressDrawable="@drawable/student_profile_body_progress_bar_circular_color" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center">

                            <LinearLayout
                                android:id="@+id/student_my_profile_earned_hours_logo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="5dp">

                                <TextView
                                    android:id="@+id/student_my_profile_earned_hours_logo_icon"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/text_logo_background_left_corner"
                                    android:text="المكتسبه"
                                    android:textColor="@android:color/white"
                                    android:textSize="18sp"
                                    android:textStyle="bold" />

                                <ImageView
                                    android:id="@+id/student_my_profile_earned_hours_logo_text"
                                    android:layout_width="27dp"
                                    android:layout_height="27dp"
                                    android:background="@drawable/ic_logo_background_right_corner"
                                    android:src="@drawable/ic_hours_white_36dp" />


                            </LinearLayout>

                            <TextView
                                android:id="@+id/student_my_profile_earned_hours_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/student_my_profile_earned_hours_logo"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:layout_marginTop="32dp"
                                android:text="0"
                                android:textColor="@android:color/black"
                                android:textSize="24sp" />

                            <ProgressBar
                                android:id="@+id/student_my_profile_earned_hours_progress_bar"
                                style="?android:attr/progressBarStyleHorizontal"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:layout_below="@+id/student_my_profile_earned_hours_logo"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/student_profile_body_progress_bar_circle_shape_background"
                                android:indeterminate="false"
                                android:max="140"
                                android:progress="0"
                                android:progressDrawable="@drawable/student_profile_body_progress_bar_circular_color" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/student_my_profile_level_logo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/text_logo_background"
                                android:text="Level"
                                android:textColor="@android:color/white"
                                android:textSize="18sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/student_my_profile_level_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/student_my_profile_level_logo"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:layout_marginTop="32dp"
                                android:text="0"
                                android:textColor="@android:color/black"
                                android:textSize="24sp" />

                            <ProgressBar
                                android:id="@+id/student_my_profile_level_progress_bar"
                                style="?android:attr/progressBarStyleHorizontal"
                                android:layout_width="80dp"
                                android:layout_height="80dp"
                                android:layout_below="@+id/student_my_profile_level_logo"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/student_profile_body_progress_bar_circle_shape_background"
                                android:indeterminate="false"
                                android:max="4"
                                android:progress="0"
                                android:progressDrawable="@drawable/student_profile_body_progress_bar_circular_color" />

                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/student_my_profile_name_ar_logo"
