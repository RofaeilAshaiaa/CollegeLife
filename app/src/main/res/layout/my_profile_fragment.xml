                        android:layout_toLeftOf="@+id/student_my_profile_national_id_logo"
                        android:layout_toStartOf="@+id/student_my_profile_national_id_logo"
                        android:text="N/A"
                        android:textColor="@android:color/black"
                        android:textSize="24sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/student_my_profile_national_id_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/student_my_profile_national_id_num"
                        android:layout_toLeftOf="@+id/student_my_profile_national_id_logo"
                        android:layout_toStartOf="@+id/student_my_profile_national_id_logo"
